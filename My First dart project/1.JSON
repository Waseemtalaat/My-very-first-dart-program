import 'dart:io';
void main ()
{
List orgmsg=[];
get_org_msg(orgmsg);
print("message after applying OPG:" + "$OPG($orgmsg)");
print("message after applying EPG:" + "$EPG($orgmsg)");







}
void get_org_msg (List x) /*take input*/
{
print("enter your message");
for(int i=0;i<=4;i++)
{
x[i]=stdin.readLineSync()!;
if(x[i] !=0 || x[i] !=1) /*check*/
{
print("error: original msg cannot contain these chrachters");
}
}
}
void OPG (List x)
{
int ones_counter=0;
for(int i=0;i<x.length;i++)
{
if(x[i]==1)
{
ones_counter ++;
}
}
if(ones_counter%2==0)
{
x.add(1);
}
else if(ones_counter%2!=0)
{
x.add(0);
}
print("original msg after append =" + x.toString());
}
void EPG (List x)
{
int ones_counter=0;
for(int i=0;i<x.length;i++)
{
if(x[i]==1)
{
ones_counter ++;
}
}
if(ones_counter%2==0)
{
x.add(0);
}
else if(ones_counter%2!=0)
{
x.add(1);
}
print("original msg after append =" + x.toString());
}
